---
import Item from '../components/item.astro';

const metoderData = [
    {title: ' Bli godt kjent med folk og steder', body: 'for å forstå behov, ønsker og drømmer', iconAlt: "Alt", iconPath: "/src/icons/defaultIcon.png" },
    {title: ' Bli godt kjent med folk og steder', body: 'for å forstå behov, ønsker og drømmer', iconAlt: "Alt", iconPath: "/src/icons/defaultIcon.png" },
    {title: ' Bli godt kjent med folk og steder', body: 'for å forstå behov, ønsker og drømmer', iconAlt: "Alt", iconPath: "/src/icons/defaultIcon.png" }
];
---

<ul class="">

    {metoderData.map(metode =>(
        <Item 
		title={metode.title}
		body={metode.body}
		imagePath={metode.iconPath}
		altText={metode.iconAlt}
		 />
     ))}

</ul>