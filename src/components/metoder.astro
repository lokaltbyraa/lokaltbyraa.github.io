---
import Item from '../components/item.astro';
import DefaultIcon from '../images/defaultIcon.png';

const metoderData = [
    {title: ' Bli godt kjent med folk og steder', body: 'for å forstå behov, ønsker og drømmer', iconSrc: DefaultIcon, iconAlt: 'Alt'},
    {title: 'Arbeide iherdig med idéer', body: 'som begeistrer og har merverdi for samfunnet', iconSrc: DefaultIcon, iconAlt: 'Alt'},
    {title: 'Teste tidlig, enkelt og ofte', body: 'for å lære fra den virkelige verden', iconSrc: DefaultIcon, iconAlt: 'Alt'}
];
---

<ul class="">

    {metoderData.map(metode =>(
        <Item title={metode.title} body={metode.body} iconSrc={metode.iconSrc} iconAlt={metode.iconAlt} />
    ))}


</ul>